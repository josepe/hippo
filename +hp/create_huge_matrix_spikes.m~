function data=create_huge_matrix_spikes()
import hp.*
di=dir('a*');
for k=2:10
    cd(di(k).name);
    
    %thalamus
    dbt=dir('t-unit*');
    dat=importdata(dbt(1).name);
    data(1,k-1).times=extractspikes(dat.data,1000);
    
    %hippocampus
    dbh=dir('h-unit*');
    dat=importdata(dbh(1).name);
    data(2,k-1).times=extractspikes(dat.data,1000);
    
    cd ..
end

end

data=extractdatapb(data,Fs,t)